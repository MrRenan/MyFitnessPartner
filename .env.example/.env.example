# Spring Profile
SPRING_PROFILE=dev

# Server Configuration
SERVER_PORT=8080

# PostgreSQL Configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=myfitnesspartner
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres

# Google Gemini Configuration
# Get your API key at: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-1.5-flash
GEMINI_MAX_TOKENS=2048
GEMINI_TEMPERATURE=0.7

# WhatsApp Business API (Meta) Configuration
# Setup guide: https://developers.facebook.com/docs/whatsapp/cloud-api/get-started
WHATSAPP_API_URL=https://graph.facebook.com/v18.0
WHATSAPP_PHONE_NUMBER_ID=your_phone_number_id_here
WHATSAPP_BUSINESS_ACCOUNT_ID=your_business_account_id_here
WHATSAPP_ACCESS_TOKEN=your_access_token_here
WHATSAPP_VERIFY_TOKEN=your_custom_verify_token_here
WHATSAPP_WEBHOOK_URL=http://localhost:8080/api/webhook/whatsapp